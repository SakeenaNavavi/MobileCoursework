<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorAccents"
    tools:context=".Register">

    <ImageView
        android:id="@+id/Registerlogo_image"
        android:layout_width="33dp"
        android:layout_height="32dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/png_clipart_female_illustration_logo_woman_women_s_beauty_center_purple_face_thumbnail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.042" />

    <TextView
        android:id="@+id/Registerfirst_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="S A F E H E R"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.175"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Registerlogo_image" />

    <TextView
        android:id="@+id/Registersecond_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Register"
        android:textColor="#F8F7FA"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Registerfirst_textView"
        app:layout_constraintHorizontal_bias="0.411" />

    <TextView
        android:id="@+id/Registerfullname_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Full Name"
        android:textColor="#F8F7FA"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Registersecond_textView" />

    <EditText
        android:id="@+id/RegistereditTextFullName"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/edittext_box_shape"
        android:hint="Full Name"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColorHint="@android:color/darker_gray"
        app:layout_constraintTop_toBottomOf="@id/Registerfullname_textView"
        tools:layout_editor_absoluteX="16dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Registeremail_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Email"
        android:textColor="#F8F7FA"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/RegistereditTextFullName" />

    <EditText
        android:id="@+id/RegistereditTextEmail"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edittext_box_shape"
        android:hint="Enter your email"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColorHint="@android:color/darker_gray"
        app:layout_constraintTop_toBottomOf="@id/Registeremail_textView"
        tools:layout_editor_absoluteX="16dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/Registerpassword_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Password"
        android:textColor="#F8F7FA"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/RegistereditTextEmail" />

    <EditText
        android:id="@+id/RegistereditTextPassword"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edittext_box_shape"
        android:hint="Password"
        android:inputType="textPassword"
        android:padding="12dp"
        android:textColorHint="@android:color/darker_gray"
        app:layout_constraintTop_toBottomOf="@id/Registerpassword_textView"
        tools:layout_editor_absoluteX="16dp"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/Registergoogle_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:layout_margin="30dp"
        android:src="@drawable/apple"
        android:background="@drawable/image_border"
        app:layout_constraintEnd_toStartOf="@id/Registerfacebook_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/RegistereditTextPassword"
        app:layout_constraintHorizontal_chainStyle="packed" />

    <ImageView
        android:id="@+id/Registerfacebook_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:layout_margin="30dp"
        android:src="@drawable/fb"
        android:background="@drawable/image_border"
        app:layout_constraintStart_toEndOf="@id/Registergoogle_icon"
        app:layout_constraintEnd_toStartOf="@id/Registerapple_icon"
        app:layout_constraintTop_toBottomOf="@id/RegistereditTextPassword"
        app:layout_constraintHorizontal_chainStyle="packed" />

    <ImageView
        android:id="@+id/Registerapple_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:layout_margin="30dp"
        android:src="@drawable/ins"
        android:background="@drawable/image_border"
        app:layout_constraintStart_toEndOf="@id/Registerfacebook_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/RegistereditTextPassword"
        app:layout_constraintHorizontal_chainStyle="packed" />


    <Button
        android:id="@+id/RegisterbuttonRegister"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/edittext_box_shape"
        android:text="Register"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Registerfacebook_icon" />

    <Button
        android:id="@+id/RegisterbuttonNext"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:background="@android:color/transparent"
        android:contentDescription="Next button"
        android:text="Next >"
        android:textColor="#FFFFFF"
        app:layout_constraintTop_toBottomOf="@id/RegisterbuttonRegister"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="295dp" />

    <TextView
        android:id="@+id/RegisterLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        android:minHeight="48dp"
        android:text="Do you have an account? Login here"
        android:textColor="#FF000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTo="@id/RegisterbuttonNext"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="634dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

