<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFCFD"
    tools:context=".AdminList">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/png_clipart_female_illustration_logo_woman_women_s_beauty_center_purple_face_thumbnail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SAFEHER"
        android:textColor="#904C77"
        android:textSize="15sp"
        android:fontFamily="@font/nunito"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/logo"
        android:layout_marginTop="20dp"
        android:layout_marginStart="60dp" />
    <TextView
        android:id="@+id/nameB1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_below="@id/tvTitle"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text=""
        android:textColor="#040002"
        android:background="#F98CAD"
        android:fontFamily="@font/nunito"
        android:gravity="center"
        android:padding="9dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/nameB2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_below="@id/tvTitle"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text=""
        android:textColor="#040002"
        android:background="#F98CAD"
        android:fontFamily="@font/nunito"
        android:gravity="center"
        android:padding="9dp"
        android:textSize="16sp" />



    <TextView
        android:id="@+id/nameB3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="175dp"
        android:layout_below="@id/tvTitle"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text=""
        android:textColor="#040002"
        android:background="#F98CAD"
        android:fontFamily="@font/nunito"
        android:gravity="center"
        android:padding="9dp"
        android:textSize="16sp" />


    <TextView
        android:id="@+id/nameB4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="325dp"
        android:layout_below="@id/tvTitle"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text=""
        android:textColor="#040002"
        android:background="#F98CAD"
        android:fontFamily="@font/nunito"
        android:gravity="left"
        android:textStyle="bold"
        android:padding="9dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/nameB5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_below="@id/tvTitle"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text=""
        android:textColor="#040002"
        android:background="#F98CAD"
        android:fontFamily="@font/nunito"
        android:gravity="left"
        android:textStyle="bold"
        android:padding="9dp"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Adming List"
        android:textColor="#070707"
        android:layout_marginTop="60dp"
        android:layout_centerHorizontal="true"
        android:textSize="24sp"
        android:fontFamily="@font/nunito" />



    <Button
        android:id="@+id/btnAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/btnGuardian"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="550dp"
        android:layout_marginRight="50dp"
        android:backgroundTint="#E91E63"
        android:fontFamily="@font/nunito"
        android:text="Back"
        android:textColor="#FFFCFD"
        android:layout_alignParentEnd="true"
        tools:ignore="NotSibling,UnknownId" />


    <View
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="#FF6492"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="621dp"/>
</RelativeLayout>


